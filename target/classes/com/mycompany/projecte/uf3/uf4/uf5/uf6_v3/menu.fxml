<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.projecte.uf3.uf4.uf5.uf6_v3.MenuController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <AnchorPane prefHeight="300.0" prefWidth="300.0">
         <children>
            <Label layoutX="73.0" layoutY="14.0" prefHeight="30.0" prefWidth="154.0" text="Caixer Automàtic">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Button fx:id="consultar" layoutX="2.0" layoutY="90.0" mnemonicParsing="false" onAction="#switchToConsultar" prefHeight="30.0" prefWidth="142.0" text="Consultar saldo" />
            <Button fx:id="retirar" layoutX="3.0" layoutY="134.0" mnemonicParsing="false" onAction="#switchToRetirar" prefHeight="30.0" prefWidth="141.0" text="Retirar diners" />
            <Button fx:id="transferencia" layoutX="2.0" layoutY="175.0" mnemonicParsing="false" onAction="#switchToTransferencia" prefHeight="30.0" prefWidth="142.0" text="Realitzar trasferencia" />
            <Button fx:id="canviarClau" layoutX="155.0" layoutY="175.0" mnemonicParsing="false" onAction="#switchToClau" prefHeight="30.0" prefWidth="141.0" text="Canviar clau d'accés" />
            <Button fx:id="logout" layoutX="79.0" layoutY="221.0" mnemonicParsing="false" onAction="#switchToLogout" prefHeight="30.0" prefWidth="142.0" text="Logout" />
            <Button fx:id="ingresar" layoutX="3.0" layoutY="52.0" mnemonicParsing="false" onAction="#switchToIngresar" prefHeight="30.0" prefWidth="142.0" text="Ingresar diners" />
            <Button layoutX="155.0" layoutY="52.0" mnemonicParsing="false" onAction="#switchToBizum" prefHeight="30.0" prefWidth="142.0" text="Bizum" />
            <Button layoutX="155.0" layoutY="90.0" mnemonicParsing="false" onAction="#switchToCrearCompte" prefHeight="30.0" prefWidth="142.0" text="Crear compte" />
            <Button layoutX="155.0" layoutY="134.0" mnemonicParsing="false" onAction="#switchToPagarFactures" prefHeight="30.0" prefWidth="142.0" text="Pagar factures" />
         </children>
      </AnchorPane>
   </children>
</VBox>
